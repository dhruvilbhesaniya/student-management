     <form class="r-form" method="post" action="/admin/addcourse" enctype='multipart/form-data'>
             {{!-- <h2 class="text-center">Ragistetion</h2> --}}
                
                    <div class="from1">
                        <div class="col-sm-8 form-group">
                            <label for="name-f">Course Name</label>
                            <input type="text" class="form-control" name="cname" value="" placeholder="Course Name">
                         </div>
                          <div class="col-sm-8 form-group">
                            <label for="Date">Starting Date</label>
                            <input type="Date" name="date" value="" class="form-control" id="Date" placeholder="Starting Date">
                        </div>
                    </div>

                   <div class="from1">
                         <div class="col-sm-8 form-group">
                        <label for="name-l">Course Duration</label>
                        <input type="text" class="form-control" name="duration" id="name-l" placeholder="Course Duration">
                    </div>
                     <div class="col-sm-8 form-group">
                            <label for="tel">Course Price</label>
                            <input type="number" name="price" class="form-control" id="tel" placeholder="Course Price">
                        </div>
                   </div>

                    <div class="from1">
                        <div class="col-sm-8 form-group">
                            <label for="address-1">Professor Name</label>
                            <input type="text" class="form-control" name="pname" id="address-1" placeholder="Professor Name">
                         </div>
