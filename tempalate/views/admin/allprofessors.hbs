<!DOCTYPE html>
<html lang="en">
<head>
    {{>adminHead}}

  {{!--
  <link rel="stylesheet" href="https://maxcdn.bootstrapcdn.com/bootstrap/3.3.7/css/bootstrap.min.css"> --}}
  
  
        <link href="https://cdn.jsdelivr.net/npm/bootstrap@5.0.2/dist/css/bootstrap.min.css" rel="stylesheet"
        integrity="sha384-EVSTQN3/azprG1Anm3QDgpJLIm9Nao0Yz1ztcQTwFspd3yD65VohhpuuCOmLASjC" crossorigin="anonymous">
         <link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/font-awesome/5.15.4/css/all.min.css"
          integrity="sha512-1ycn6IcaQQ40/MKBW2W4Rhis/DbILU74C1vSrLJxCq57o941Ym01SwNsOMqvEBFlcgUa6xLiPY/NS5R+E6ztJQ=="
        crossorigin="anonymous" referrerpolicy="no-referrer" />

        <link href="https://cdn.jsdelivr.net/npm/bootstrap@5.0.0-beta2/dist/css/bootstrap.min.css" rel="stylesheet" crossorigin="anonymous">
        <script src="https://cdn.jsdelivr.net/npm/bootstrap@5.0.0-beta2/dist/js/bootstrap.bundle.min.js"crossorigin="anonymous"></script>

</head>
<body style="background-color:#edf2f6 ;">
         
     {{>adminHeder}}

      <div style="position:fixed ;">
        {{>adminNev}}
      </div>


     <div class="dashboard">
          <div class="d1">
            <h4>All Professors</h4>
         </div>

         <div class="d-btn">
              <i class="fa fa-home" style="color:#AAAAAA ;"></i>
               <a href="index">Home </a>
               <i class="fa fa-angle-right" style="color:#AAAAAA ;"></i>
               <span >All Professors</span>
         </div>
        
     </div>


    <div class="student-table">
            <h2>All Professors List</h2>
            <div class="add-btn1">
                <a href="addprofessors"> Add New <i class="fa fa-plus"></i></a>
            </div>

            <table class="table table-striped table-bordered table-hover table-checkable order-column valign-middle dataTable no-footer" aria-describedby="example4_info" style="width: 1230px; margin-left: 20px;border: 1px solid rgb(208, 198, 198); margin-top: 20px;">
                <thead>
                    <tr>
                        {{!-- <th style="width: 70px;">No</th> --}}
                        <th style="width: 240px;">Name</th>
                        <th style="width: 240px;">Email</th>
                        <th style="width: 120px;">Phone</th>
                        <th style="width: 110px;">Joining Date</th>
                        <th style="width: 130px;">Department</th>
                        <th style="width: 130px;">Degree</th>
                        <th style="width: 100px;">Action</th>
                    </tr>
                </thead>
                 <tbody>
                       {{#each pdataget}}
                        <tr>
                            
                            {{!-- <td>{{this.no}}</td> --}}
                            <td>{{this.first_name}} {{this.last_name}}</td>
                            <td>{{this.Email}}</td>
                            <td>{{this.phone}}</td>
                            <td>{{this.Joining_date}}</td>
                            <td>{{this.Departments}}</td>
                            <td>{{this.Degree}}</td>
                            <td>
                                <div class="table-btn">
                                    <a href="editprofessors/{{this._id}}" class="table-edit-btn"><i class="fas fa-pencil-alt" ></i></a>
                                    <a href="allprofessors/{{this._id}}"  class="table-delet-btn"><i class="fas fa-trash-alt"></i></a>
                                </div>  
                            </td>
                        </tr>
                        {{/each}}  
                 </tbody>
            </table>
            
    </div>

</body>
</html>