<!DOCTYPE html>
<html lang="en">

<head>
    {{>adminHead}}

    {{!--
    <link rel="stylesheet" href="https://maxcdn.bootstrapcdn.com/bootstrap/3.3.7/css/bootstrap.min.css"> --}}


    <link href="https://cdn.jsdelivr.net/npm/bootstrap@5.0.2/dist/css/bootstrap.min.css" rel="stylesheet"
        integrity="sha384-EVSTQN3/azprG1Anm3QDgpJLIm9Nao0Yz1ztcQTwFspd3yD65VohhpuuCOmLASjC" crossorigin="anonymous">
    <link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/font-awesome/5.15.4/css/all.min.css"
        integrity="sha512-1ycn6IcaQQ40/MKBW2W4Rhis/DbILU74C1vSrLJxCq57o941Ym01SwNsOMqvEBFlcgUa6xLiPY/NS5R+E6ztJQ=="
        crossorigin="anonymous" referrerpolicy="no-referrer" />

    <link href="https://cdn.jsdelivr.net/npm/bootstrap@5.0.0-beta2/dist/css/bootstrap.min.css" rel="stylesheet"
        crossorigin="anonymous">
    <script src="https://cdn.jsdelivr.net/npm/bootstrap@5.0.0-beta2/dist/js/bootstrap.bundle.min.js"
        crossorigin="anonymous"></script>

</head>

<body style="background-color:#edf2f6 ;">

    {{>adminHeder}}

    <div style="position:fixed ;">
        {{>adminNev}}
    </div>


    <div class="dashboard">
        <div class="d1">
            <h4>All Students</h4>
        </div>

        <div class="d-btn">
            <i class="fa fa-home" style="color:#AAAAAA ;"></i>
            <a href="index">Home </a>
            <i class="fa fa-angle-right" style="color:#AAAAAA ;"></i>
            <span> All Students</span>
        </div>

    </div>


    <div class="student-table">
        <h2>All Students List</h2>
        <div class="add-btn1">
            <a href="addstudents"> Add New <i class="fa fa-plus"></i></a>
        </div>


            <form action="/admin/allstudents" method="">
                <div class="s-button">
                        <div class="s-form">
                            <input type="search" name="name" id="" class="form-control" placeholder="Search By Student Name, Address, course, etc.. "/>
                            {{!-- <input type="search" name="Addres" id="" class="form-control" placeholder="Search By Address"/> --}}
                            {{!-- <input type="search" name="Courses" id="" class="form-control" placeholder="Search By Course"/> --}}
                            {{!-- <select type="search" name="Courses" value="" class="form-control custom-select browser-default">
                                        <option value="Bsc.it">Bsc.it</option>
                                        <option value="BCA">BCA</option>
                                        <option value="Bse">Bse</option>
                                        <option value="B.com">B.com</option>
                                        <option value="BBA">BBA</option>
                                    </select> --}}
                        </div>

                        <button type="submit" value="" class="btn btn-primary">
                            {{!-- <a href="allstudents/{{dataget.Addres}}"> --}}
                            <i class="fas fa-search"></i>

                            </a>
                        </button>
            
                </div>
            </form >

        <table
            class="table table-striped table-bordered table-hover table-checkable order-column valign-middle dataTable no-footer"
            aria-describedby="example4_info"
            style="width: 1230px; margin-left: 20px;border: 1px solid rgb(208, 198, 198); margin-top: 20px;"
            id="student-table">
            <thead>
                <tr>
                    {{!-- <th style="width: 70px;">Rollno</th> --}}
                    <th style="width: 270px;">Name</th>
                    <th style="width: 270px;">Email</th>
                    <th style="width: 100px;">Gender</th>
                    <th style="width: 140px;">Address</th>
                    <th style="width: 110px;">Phone</th>
                    <th style="width: 130px;">courses</th>
                    <th style="width: 100px;">Action</th>
                </tr>
            </thead>
            <tbody>
                {{#each dataget}}
                <tr>

                    {{!-- <td>{{this.rollno}}</td> --}}
                    <td>{{this.first_name}} {{this.midal_name}} {{this.last_name}}</td>
                    <td>{{this.Email}}</td>
                    <td>{{this.gendre}}</td>
                    <td>{{this.Addres}}</td>
                    <td>{{this.phone}}</td>
                    <td>{{this.Courses}}</td>
                    <td>
                        <div class="table-btn">
                            <a href="editstudents/{{this._id}}" class="table-edit-btn"><i
                                    class="fas fa-pencil-alt"></i></a>
                            <a href="allstudents/{{this._id}}" class="table-delet-btn"><i
                                    class="fas fa-trash-alt"></i></a>
                        </div>
                    </td>
                </tr>
                {{/each}}
            </tbody>
        </table>

    </div>

</body>

</html>